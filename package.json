{
  "name": "nginxgui_for_mac",
  "version": "1.0.0",
  "author": "zoeblow",
  "license": "ISC",
  "description": "electron开发的nginx图形管理工具Mac版",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "pack:mac": "electron-packager ./ NginxGui --mas --out ../ --ignore package-lock.json --ignore node_modules --ignore src --ignore webpack.config.js --ignore favicon.ico --ignore README.md --ignore .gitignore"
  },
  "build": {
    "win": "favicon.ico"
  },
  "keywords": [
    "electron", "Mac", "gui", "nginx"
  ],
  "devDependencies": {
    "antd": "^3.6.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.8.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bable-loader": "^0.0.1-security",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "electron": "^2.0.0",
    "electron-packager": "^12.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "resolve-url-loader": "^2.3.0",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoeblow/NginxGui_For_Mac.git"
  },
  "bugs": {
    "url": "https://github.com/zoeblow/NginxGui_For_Mac/issues"
  },
  "homepage": "https://github.com/zoeblow/NginxGui_For_Mac#readme"
}